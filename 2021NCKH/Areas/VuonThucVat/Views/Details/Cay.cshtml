@model IEnumerable<_2021NCKH.Models.ThucVat>
@{
    ViewBag.Title = "Cay";
    Layout = "~/Areas/VuonThucVat/Views/Shared/_Layout.cshtml";
}

<h2>Cây Thực vật</h2>

<table class="table">
    <tr>
        <th>
            Tên cây thực vật
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                <a href="@Url.Action("Details","ThucVats1", new { id = item.MaCay })">
                    @item.TenVietNam
                </a>
            </td>
        </tr>
    }
    <tr>
        <td>
            <a href="@Url.Action("Loai")">
                Loài
            </a>
        </td>
    </tr>
</table>
