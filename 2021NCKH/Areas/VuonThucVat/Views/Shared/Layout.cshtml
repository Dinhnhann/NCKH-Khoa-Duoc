@model IEnumerable<_2021NCKH.Models.ThucVat>
@{ string nganh = "";
                string lop = "";
                string phanlop = "";
                string bo = "";
                string ho = "";
                string chi = "";
                string loai = ""; }
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>Startmin - Bootstrap Admin Theme</title>

    <!-- Bootstrap Core CSS -->
    <link href="@Url.Content("~/assets/css/bootstrap.min.css")" rel="stylesheet">

    <!-- MetisMenu CSS -->
    <link href="@Url.Content("~/assets/css/metisMenu.min.css")" rel="stylesheet">

    <!-- Custom CSS -->
    <link href="@Url.Content("~/assets/css/startmin.css")" rel="stylesheet">

    <!-- Custom Fonts -->
    <link href="@Url.Content("~/assets/css/font-awesome.min.css")" rel="stylesheet" type="text/css">

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body>

    <div id="wrapper">

        <!-- Navigation -->
        <nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
            <div class="navbar-header">
                <a class="navbar-brand" href="index.html">Startmin</a>
            </div>

            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>

            <ul class="nav navbar-nav navbar-left navbar-top-links">
                <li><a href="#"><i class="fa fa-home fa-fw"></i>Khoa Dược</a></li>
            </ul>

            <ul class="nav navbar-right navbar-top-links">
                <li class="dropdown navbar-inverse">
                    <a class="dropdown-toggle" data-toggle="dropdown" href="#">
                        <i class="fa fa-bell fa-fw"></i><b class="caret"></b>
                    </a>
                </li>
            </ul>
            <!-- /.navbar-top-links -->

            <div class="navbar-default sidebar" role="navigation">
                <div class="sidebar-nav navbar-collapse">
                    <ul class="nav" id="side-menu">
                        @foreach (var item in Model)
                        {
                            <li>
                                @if (item.LoaiThucVat.ChiThucVat.HoThucVat.BoThucVat.PhanLopThucVat.LopThucVat.NganhThucVat.TenNganhThucVat != nganh)
                                {
                                    nganh = @item.LoaiThucVat.ChiThucVat.HoThucVat.BoThucVat.PhanLopThucVat.LopThucVat.NganhThucVat.TenNganhThucVat;
                                <a href="#"><i class="fa fa-sitemap fa-fw"></i>Ngành @nganh<span class="fa arrow"></span></a>
                                <ul class="nav nav-second-level">
                                    @foreach (var item2 in Model)
                                    {
                                    <li>
                                        @if (item2.LoaiThucVat.ChiThucVat.HoThucVat.BoThucVat.PhanLopThucVat.LopThucVat.NganhThucVat.TenNganhThucVat == nganh && item2.LoaiThucVat.ChiThucVat.HoThucVat.BoThucVat.PhanLopThucVat.LopThucVat.TenLopThucVat != lop)
                                        {
                                            lop = @item2.LoaiThucVat.ChiThucVat.HoThucVat.BoThucVat.PhanLopThucVat.LopThucVat.TenLopThucVat;
                                        <a href="#">Lớp @lop</a>
                                        <ul class="nav nav-second-level">
                                            @foreach (var item3 in Model)
                                            {
                                            <li>
                                                @if (item3.LoaiThucVat.ChiThucVat.HoThucVat.BoThucVat.PhanLopThucVat.LopThucVat.TenLopThucVat == lop && item3.LoaiThucVat.ChiThucVat.HoThucVat.BoThucVat.PhanLopThucVat.TenPhanLopThucVat != phanlop)
                                                {
                                                    phanlop = @item3.LoaiThucVat.ChiThucVat.HoThucVat.BoThucVat.PhanLopThucVat.TenPhanLopThucVat;
                                                    <a href="#">Phân lớp @phanlop</a>
                                                    <ul class="nav nav-second-level">
                                                    @foreach (var item4 in Model)
                                                    {
                                                        <li>
                                                            @if (item4.LoaiThucVat.ChiThucVat.HoThucVat.BoThucVat.PhanLopThucVat.TenPhanLopThucVat == phanlop && item4.LoaiThucVat.ChiThucVat.HoThucVat.BoThucVat.TenBoThucVat != bo)
                                                            {
                                                                bo = @item4.LoaiThucVat.ChiThucVat.HoThucVat.BoThucVat.TenBoThucVat;
                                                                <a href="#">Bộ @bo</a>
                                                                <ul class="nav nav-second-level">
                                                                @foreach (var item5 in Model)
                                                                {
                                                                    <li>
                                                                    @if (item5.LoaiThucVat.ChiThucVat.HoThucVat.BoThucVat.TenBoThucVat == bo && item5.LoaiThucVat.ChiThucVat.HoThucVat.TenHoThucVat != ho)
                                                                    {
                                                                        ho = @item5.LoaiThucVat.ChiThucVat.HoThucVat.TenHoThucVat;
                                                                        <a href="#">Họ @ho</a>
                                                                        <ul class="nav nav-second-level">
                                                                        @foreach (var item6 in Model)
                                                                        {
                                                                            <li>
                                                                            @if (item6.LoaiThucVat.ChiThucVat.HoThucVat.TenHoThucVat == ho && item6.LoaiThucVat.ChiThucVat.TenChiThucVat != chi)
                                                                            {
                                                                                chi = @item6.LoaiThucVat.ChiThucVat.TenChiThucVat;
                                                                                <a href="#">Chi @chi</a>
                                                                                <ul class="nav nav-second-level">
                                                                                    @foreach (var item7 in Model)
                                                                                    {
                                                                                        <li>
                                                                                        @if (item7.LoaiThucVat.ChiThucVat.TenChiThucVat == chi && item7.LoaiThucVat.TenLoaiThucVat != loai)
                                                                                        {
                                                                                            loai = @item7.LoaiThucVat.TenLoaiThucVat;
                                                                                            <a href="#">Loài @loai</a>}
                                                                                        </li>}
                                                                                    </ul>}
                                                                                </li>}
                                                                            </ul>}
                                                                        </li>}
                                                                    </ul>}
                                                                </li>}
                                                            </ul>}
                                                        </li>}
                                                    </ul>}
                                                </li>}
                                            </ul>}
                                        </li>}
                    </ul>
                </div>
                <!-- /.sidebar-collapse -->
            </div>
            <!-- /.navbar-static-side -->
        </nav>

        <!-- Page Content -->
        <div id="page-wrapper">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-lg-12">
                        @RenderBody()
                    </div>
                    <!-- /.col-lg-12 -->
                </div>
                <!-- /.row -->
            </div>
            <!-- /.container-fluid -->
        </div>
        <!-- /#page-wrapper -->

    </div>
    <!-- /#wrapper -->
    <!-- jQuery -->
    <script src="@Url.Content("~/assets/js/jquery.min.js")"></script>

    <!-- Bootstrap Core JavaScript -->
    <script src="@Url.Content("~/assets/js/bootstrap.min.js")"></script>

    <!-- Metis Menu Plugin JavaScript -->
    <script src="@Url.Content("~/assets/js/metisMenu.min.js")"></script>

    <!-- Custom Theme JavaScript -->
    <script src="@Url.Content("~/assets/js/startmin.js")"></script>

</body>
</html>
