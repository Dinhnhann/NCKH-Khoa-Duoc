<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>Khoa Dược - Trường Đại Học Văn Lang</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="manifest" href="site.webmanifest">
    <link rel="shortcut icon" type="image/x-icon" href="@Url.Content("~/assets/img/NCKH img/header-logo/img-logo.png")">
    @Styles.Render("~/assets/css")
    @if (IsSectionDefined("css"))
    {
        @RenderSection("css", required: false)
    }
    <!-- CSS here -->
    <link rel="stylesheet" href="@Url.Content("~/assets/css/bootstrap.min.css")">
    <link rel="stylesheet" href="@Url.Content("~/assets/css/owl.carousel.min.css")">
    <link rel="stylesheet" href="@Url.Content("~/assets/css/slicknav.css")">
    <link rel="stylesheet" href="@Url.Content("~/assets/css/animate.min.css")">
    <link rel="stylesheet" href="@Url.Content("~/assets/css/magnific-popup.css")">
    <link rel="stylesheet" href="@Url.Content("~/assets/css/fontawesome-all.min.css")">
    <link rel="stylesheet" href="@Url.Content("~/assets/css/themify-icons.css")">
    <link rel="stylesheet" href="@Url.Content("~/assets/css/slick.css")">
    <link rel="stylesheet" href="@Url.Content("~/assets/css/nice-select.css")">
    <link rel="stylesheet" href="@Url.Content("~/assets/css/style.css")">
</head>
<body>
    @Html.Partial("~/Views/Shared/Common/Header.cshtml")
    <div class="main-content">
        @if (TempData["AMess"] != null)
        {
            <div id="noticeAlert" class="alert @TempData["AType"] hide" style="margin-top:30px">
                @TempData["AMess"]
            </div>
        }
        @RenderBody()
        @Html.Partial("~/Views/Shared/Common/Footer.cshtml")


        <script src="@Url.Content("~/./assets/js/vendor/modernizr-3.5.0.min.js")"></script>

        <!-- Jquery, Popper, Bootstrap -->
        <script src="@Url.Content("~/./assets/js/vendor/jquery-1.12.4.min.js")"></script>
        <script src="@Url.Content("~/./assets/js/popper.min.js")"></script>
        <script src="@Url.Content("~/./assets/js/bootstrap.min.js")"></script>
        <!-- Jquery Mobile Menu -->
        <script src="@Url.Content("~/./assets/js/jquery.slicknav.min.js")"></script>

        <!-- Jquery Slick , Owl-Carousel Plugins -->
        <script src="@Url.Content("~/./assets/js/owl.carousel.min.js")"></script>
        <script src="@Url.Content("~/./assets/js/slick.min.js")"></script>
        <!-- One Page, Animated-HeadLin -->
        <script src="@Url.Content("~/./assets/js/wow.min.js")"></script>
        <script src="@Url.Content("~/./assets/js/animated.headline.js")"></script>
        <script src="@Url.Content("~/./assets/js/jquery.magnific-popup.js")"></script>

        <!-- Scrollup, nice-select, sticky -->
        <script src="@Url.Content("~/./assets/js/jquery.scrollUp.min.js")"></script>
        <script src="@Url.Content("~/./assets/js/jquery.nice-select.min.js")"></script>
        <script src="@Url.Content("~/./assets/js/jquery.sticky.js")"></script>

        <!-- contact js -->
        <script src="@Url.Content("~/./assets/js/contact.js")"></script>
        <script src="@Url.Content("~/./assets/js/jquery.form.js")"></script>
        <script src="@Url.Content("~/./assets/js/jquery.validate.min.js")"></script>
        <script src="@Url.Content("~/./assets/js/mail-script.js")"></script>
        <script src="@Url.Content("~/./assets/js/jquery.ajaxchimp.min.js")"></script>

        <!-- Jquery Plugins, main Jquery -->
        <script src="@Url.Content("~/./assets/js/plugins.js")"></script>
        <script src="@Url.Content("~/./assets/js/main.js")"></script>

        @if (IsSectionDefined("scripts"))
        {
            @RenderSection("scripts", required: false)
        }
</body>
</html>
